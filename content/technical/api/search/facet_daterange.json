{
    "aggregations": {
        "type": "date_range",
        "field": "book.publish_date",
        "format": "date",
        "ranges": [
            {
                // all books published in the 20th century
                "to": "1999-12-31",
                "from": "1900-01-01"
            },
            {
                // all books published in the 19th century
                "to": "1899-12-31",
                "from": "1800-01-01"
            }
        ]
    }
}